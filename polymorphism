class Games{
  public void favouriteplayer() {
    System.out.println(" My Favourite Player");
  }
}

class cricket extends Games {
  public void favouriteplayer() {
    System.out.println(" cricket : Dhoni");
  }
}

class football extends Games {
  public void favouriteplayer() {
    System.out.println("football: Messi");
  }
}

class Main {
  public static void main(String[] args) {
     Games Games = new Games();
     Games cricket = new cricket();
    Games football = new football();
        
    Games.favouriteplayer();
    cricket.favouriteplayer();
    football.favouriteplayer();
  }
}
